  "scripts": {
    "ng": "ng",
    "start:localtest": "yarn start --host localtest.me --ssl true --ssl-cert ssl/cert.pem --ssl-key ssl/key.pem",
    "start:secure": "yarn start --host electronics-spa.e2y.com --ssl true --ssl-cert ssl/cert.pem --ssl-key ssl/key.pem",
    "start": "yarn build && ng serve",
    "build": "yarn build-translations && yarn build-libs",
    "build-prod": "yarn build-translations --configuration production && yarn build-libs --configuration production",
    "build-ci": "yarn build-prod && ng build example-storefront -c ci --base-href /spartacus/",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadless",
    "test:coverage": "yarn test:headless --code-coverage --watch false",
    "ci-test": "ng test checkout-spartacus-connector --code-coverage --watch false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix && yarn lint-style:fix",
    "lint-style": "stylelint \"**/*.scss\"",
    "lint-style:fix": "stylelint \"**/*.scss\" --fix",
    "build-libs": "ng build checkout-spartacus-connector",
    "watch-libs": "yarn build-libs --watch",
    "build-translations": "ng build checkout-spartacus-translations",
    "swagger": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && npx openapi-typescript@5.3.0 https://localhost:9002/occ/v2/api-docs --output projects/checkout-spartacus-connector/generated/src/occ.ts"
  },
